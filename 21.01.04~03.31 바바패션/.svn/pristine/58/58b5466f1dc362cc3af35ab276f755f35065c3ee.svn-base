.tagSet{overflow:hidden}
.tagSet > .inner{display:flex;flex-wrap: wrap;margin:-3px;}
.tagSet > .inner > span{display:inline-block;background:#eee;height: 18px;line-height: 18px;font-size:10px;color:#666;padding:0 5px;border-radius: 3px;margin: 3px;}
.tagSet-thumb > span.txt{align-items: center;color: #fff;font-size:10px;background: #111;height: 22px;line-height: 22px;border-radius: 11px;padding:0 10px;box-sizing:border-box;font-weight: 300;}
.tagSet-thumb > span.txt:not(:first-child){margin-left:5px}
