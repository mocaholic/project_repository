.countUi > input{display: block;width: 100%;border:0;text-align:center;height: 100%;padding: 0;vertical-align: top;box-sizing: border-box;font-weight: 700;background: transparent;}
.btn-minus{position: absolute;top: 0;left: 0;display:inline-block;width: 28px;height: 28px;text-indent:-10000px;background: url(../img/minus.png) no-repeat center;background-size: 10px;}
.btn-plus{position: absolute;top: 0;right: 0;display:inline-block;width: 28px;height: 28px;text-indent:-10000px;background: url(../img/plus.png) no-repeat center;background-size: 10px;}

