@charset "utf-8";
.tab-a{}
.tab-a > ul{display:flex;padding:5px 0;}
.tab-a > ul > li:not(:first-child){margin-left:28px}
.tab-a > ul > li{font-weight:200;color:#d0d0d0;margin-bottom:-1px}
.tab-a > ul > li > a{display:block;padding:10px 0 7px 0;font-weight: 300;}
.tab-a > ul > li.on{font-weight:400;color:#000}
.tab-a > ul > li.on > a{border-bottom:2px solid #000;font-weight: 500;}

.tab-a.swiper{overflow:hidden}
.tab-a.swiper > ul > li{flex:none;white-space: nowrap;display: inline-block;width: auto;}
.tab-a.swiper > ul > li > a{/* padding: 5px 0; */box-sizing: border-box;}

.tab-b {height: 56px;font-size:14px;color:#fff;padding: 0 20px}
.tab-b > ul{display: flex;}
.tab-b > ul > li{flex: auto;}
.tab-b > ul > li.on > a{color: #000;}
.tab-b > ul > li.on > a:after{content:"";display:inline-block;width:4px;height:4px;background: #000;border-radius: 50%;position: absolute;left: calc(50% - 2px);bottom: 10px;}
.tab-b > ul > li > a{position: relative;display:flex;align-items: center;justify-content: center;height: 58px;color: #999;}
.tab-b.swiper{overflow:hidden}
.tab-b.swiper > ul > li{flex:none;white-space: nowrap;display: inline-block;width: auto;}
.tab-b.swiper > ul > li > a{padding: 0 12px;box-sizing: border-box;}
.tab-b.swiper{padding: 0 8px;}

.tab-c {height:58px;font-size:14px;background:#000;color:#fff}
.tab-c > ul{display:flex}
.tab-c > ul > li{flex:1;}
.tab-c > ul > li.on > a{color:#fff;}
.tab-c > ul > li > a{display:flex;align-items: center;justify-content: center;height: 58px;color: #999;}

.tab-d{padding:15px 20px}
.tab-d > ul{display:flex;}
.tab-d > ul > li{display:inline-block;flex:none}
.tab-d > ul > li:not(:first-child){margin-left: 12px;}
.tab-d > ul > li > a{display:flex;align-items: center;justify-content: center;height: 32px;color: #666;border: .5px solid #d0d0d0;padding: 6px 12px;border-radius: 16px;box-sizing: border-box;font-size: 14px;}
.tab-d > ul > li.on > a{background:#000;color:#fff;border: .5px solid #000;}
.tab-d.swiper{overflow:hidden}
.tab-d.swiper > ul > li{width:auto}

.filterSelect-category li input[type *= 'checkbox']{display:none;left: unset;opacity: 1;}
.filterSelect-category li input[type *= 'checkbox']:checked + span{color:#b1863c;}
.filterSelect-category li input[type *= 'checkbox']:checked + span:after{display:inline;content:"✓";line-height:0;margin-left: 5px;}
.filterSelect-category > ul > li ul{margin-left:16px}
.filterSelect-category > ul > li > label{font-weight:500}
.filterSelect-category > ul > li ul{display:none}
.filterSelect-category label{display:inline-block;padding:10px 0}
.filterSelect-category li{position:relative}
.filterSelect-category > ul > li > ul > li > ul > li{font-size:14px;color:#666}
.filterSelect-category > ul > li:not(:first-child){margin-top:24px}
.filterSelect-category input[type = 'checkbox'].btn-expend{position: absolute;display:inline-block;right: 0;top: 6px;}
.filterSelect-category input[type = 'checkbox'].btn-expend:checked + ul{display:block}

.searchBox-a{position:relative}
.searchBox-a > input{position: relative;height:40px;font-size:12px;border-radius: 0;border: 1px solid #eaeaea;width:100%;padding: 0 12px;font-size: 14px;box-sizing: border-box;z-index: 5;}
.searchBox-a > .btn-del{position:absolute;right:38px;top:50%;transform: translateY(-50%);width:24px;height:24px;text-indent:-10000px;z-index: 7;}
.searchBox-a > .btn-search{position:absolute;right:8px;top:50%;transform: translateY(-50%);width:24px;height:24px;text-indent:-10000px;z-index: 8;}
.searchBox-a > .dropdownBox{position:absolute;top: 40px;background:#fff;width: 100%;display:none;z-index: 6;}
.searchBox-a > .dropdownBox > a{display:block;padding:12px;font-size:14px}
.searchBox-a > .dropdownBox > a > strong{font-weight:500}
.searchBox-a > .dropdownBox > a > strong > em{color:#b1863c}
.searchBox-a > .dropdownBox > a > span{margin-left:8px;font-weight:300}
.searchBox-a.open{box-shadow: 0 2px 8px 0 rgba(0, 0, 0, 0.12);}
.searchBox-a.open > input{border:1px solid}
.searchBox-a.open > .dropdownBox{display:block;box-shadow: 0 2px 8px 0 rgba(0, 0, 0, 0.12);padding:8px 0}
.searchBox-a .deem{display:none;position:fixed;top: 0;left:0;right:0;bottom:0;background: rgba(255,255,255,.8);z-index: 3;}
.searchBox-a.open .deem{display:block}

.findAlpabet li > label{display:block;color:#999;font-size:12px;margin:12px 0}
.findAlpabet li > a{display:block;margin:20px 0}
.findAlpabet li > a > strong{font-weight:500;font-size:14px}
.findAlpabet li > a > span{margin-left:8px;font-size: 12px}

.bottomToast{position:fixed;left:0;right:0;bottom:0;display:flex;height:64px;background:#000;color:#fff;z-index: 4;}
.bottomToast .reset{width:64px;height:64px;background:#222;color:#fff}
.bottomToast > strong{display:flex;flex:1;justify-content:flex-end;align-items:center;padding:5px 24px;font-weight:300}

.selectUI-a{overflow:hidden}
.selectUI-a .inner{display:flex;flex-wrap:wrap;margin: -3px;}
.selectUI-a label {min-width: calc(50% - 8px);max-width: calc(50% - 8px);min-height: 48px;box-sizing: border-box;margin: 3px;}
.selectUI-a.n3 label {min-width: calc(33.3% - 8px);height:48px;box-sizing: border-box;margin: 4px;}
.selectUI-a label > input{display:none}
.selectUI-a label > span{display: flex;flex-wrap: wrap;justify-content: center;align-items: center;width: 100%;height: 100%;border:1px solid #eaeaea;box-sizing: border-box;text-align: center;font-size: 14px;padding: 14px 6px;}
.selectUI-a label > span > i{letter-spacing: -1px;}
.selectUI-a label > span > i > em{font-size:12px}
.selectUI-a label > input:disabled + span{background:#eaeaea;border-color:#eaeaea;color:#999}
.selectUI-a label > input:checked + span{border-color: #b1863c;}
.customInputRange + .selectUI-a{margin-top:48px}

.selectUI-color .inner{display:flex;flex-wrap:wrap;}
.selectUI-color .inner > label{width:25%;margin: 10px 0;}
.selectUI-color label > span{position: relative;display:block;font-size: 14px;text-align: center;}
.selectUI-color label > input{display:none}
.selectUI-color label > span > em{position:relative;display:block;width: 32px;height: 32px;border-radius:50%;box-sizing: border-box;margin: 5px auto 5px auto;}
.selectUI-color label > input:checked + span{font-weight: 600;color:#b1863c;}
.selectUI-color label > input:checked + span:after{content:"";position:absolute;left: calc(50% - 20px);top: -4px;display:block;width: 40px;height: 40px;border-radius:50%;border: 1px solid #b1863c;box-sizing: border-box;}


.tagSet-a{display:flex;}
.tagSet-a > .inner{margin:-6px}
.tagSet-a > .inner > span{display:inline-block;height:28px;line-height:28px;border-radius:14px;padding: 0 12px;border: 1px solid;font-size:14px;margin:6px;vertical-align: middle;}
.tagSet-a > .inner > span .del{display:inline-block;width:12px;height:12px;text-indent:-10000px;vertical-align:middle;margin:-2px 0 0 5px}

.customInputRange button{width:100%;height:48px;background:#fff;border:1px solid;margin-top:12px;box-sizing:border-box;font-size:16px}
.customInputRange button:disabled{border:1px solid #eaeaea;pointer-events:none}
.fieldSet{display:flex;align-items:center;color:#999}
.fieldSet input{border:1px solid #eaeaea;height:48px;box-sizing:border-box;padding:0 20px;width: 50%;font-size:14px}
.fieldSet input:first-child{margin-right:10px}
.fieldSet input:last-child{margin-left:10px}

.layerPop-filter{position:fixed;top:0;left:0;right:0;bottom:0;}
.layerPop-filter:before{content:"";display:block;background: #f8f8f8;width:120px;height:100%;position: absolute;z-index: 0;}
.layerPop-filter .header{position:relative;z-index: 2;background: #fff;box-shadow:0 2px 4px 0 rgba(0, 0, 0, 0.06);}
.layerPop-filter h1{font-size:18px;font-weight:300;height:56px;line-height:56px;padding:0 20px}
.layerPop-filter > .cont{position:relative;z-index: 1;height: calc(100% - 114px);}
.layerPop-filter > .cont > ul > li > button{position: relative;width:120px;height:60px;color:#999;font-size:14px;text-align:left;padding:0 20px;background: #f8f8f8;box-sizing:border-box;z-index: 2;}
.layerPop-filter > .cont > ul > li > input[type='radio'] {display:none}
.layerPop-filter > .cont > ul > li > label{display: inline-block;position: relative;width:120px;height:60px;line-height: 60px;color:#999;font-size:14px;text-align:left;padding:0 20px;background: #f8f8f8;box-sizing:border-box;cursor: pointer;z-index: 2;}
.layerPop-filter > .cont > ul > li > input:checked + label{background:#fff;color:#000;font-weight:600}
.layerPop-filter > .cont > ul > li > input:checked + label:after{content:"✓";margin-left:7px}
.layerPop-filter > .cont > ul > li > input:checked + label + div{display:block;}
.layerPop-filter > .cont > ul > li > label > input{display:none}
.layerPop-filter > .cont > ul > li > div{display:none}
.layerPop-filter > .cont > ul > li.on > div{display:block}
.layerPop-filter > .cont > ul > li.on > button{background:#fff;color:#000;font-weight:600}
.layerPop-filter > .cont > ul > li.on > button:after{content:"✓";margin-left:7px}
.layerPop-filter .btn-close{position:absolute;right:16px;top:16px;width:32px;height:32px;text-indent:-10000px;z-index: 3;}
.layerPop-filter .filerTab-cont{position: absolute;top:0;bottom: 0;padding: 20px 20px 20px 140px;width: 100%;height: 100%;overflow-y: auto;box-sizing: border-box;}
.title-filter{font-size:16px}
.title-filter + *{margin-top:20px}
* + .title-filter{margin-top: 48px}

.optionSelectView{padding:14px 20px}

.inpToBtn {position:relative}
.inpToBtn button{position: absolute;top: 8px;right: 8px;height:30px;line-height: 30px;padding: 0 20px;background: #e9e9e9;color: #666;min-width: 80px;border-radius: 4px;font-size: 11px;}
.inpToBtn.type2 > input{text-align:right;font-size: 16px;font-weight: 500;}
.inpToBtn > input{padding-right:100px}

.btnWrap{display:flex;margin: 20px 0;}
