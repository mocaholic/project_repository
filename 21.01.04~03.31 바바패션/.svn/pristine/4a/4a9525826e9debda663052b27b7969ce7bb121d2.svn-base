                    </div>
                </div>
            </div>
            <!-- ///banner-type2 -->

            <script>
                var swiper = new Swiper('.banner-type2 .swiper-container', {
                    slidesPerView: 2,
                    spaceBetween: 10,
                    freeMode: true,
                    pagination:{
                        el: '.swiper-pagination',
                    },
                });
            </script>
        </div>

        <div class="guide-wrap">
