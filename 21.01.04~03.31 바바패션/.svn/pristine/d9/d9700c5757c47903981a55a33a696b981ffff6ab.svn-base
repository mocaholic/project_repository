/*LayerPopup*/
.layerPop{
	position:fixed;top:0;left:0;right:0;bottom:0;width:100%;height:100%;z-index:100;
	background:rgba(17,17,17,0.5);transform:translateX(-100%);visibility:hidden;
	display:flex;justify-content:center;align-items:center;overflow:hidden;
}
.layerPop .cont{
	position:relative;background:#fff; padding:30px; border-radius:16px;
	box-shadow:3px 47px 109px rgba(7,7,7,0.15); overflow:hidden;
	max-height:calc(100vh - 100px);
}
.layerPop .w-400 {width:400px;}
.layerPop .w-440 {width:440px;}
.layerPop .w-500 {width:500px;}
.layerPop .btn-close{position:absolute; top:30px; right:30px; width:20px; height:20px; background:url('../img/btn_closed.png') center center no-repeat; font-size:0;}
.layerPop.open{transform:translateX(0);visibility:visible;}
.layerTitle {font-size:16px; color:#111; font-weight:400; line-height:20px; margin-bottom:20px;}
.layerBody {padding-top:10px; font-size:14px;}
.layerPop .btn-bottom-group{margin-top:40px; font-size:0; text-align:center;} 
.layerPop .btn-bottom-group .btn + .btn {margin-left:10px;}

/*tab*/
.tab-ui ul{display:flex}
.tab-ui li a{display:flex;justify-content:center;align-items:center; text-decoration:none;}
.tab-cont > div{display:none}
.tab-cont > div.on{display:block}

.tab-a {margin-bottom:20px;}
.tab-a li + li {margin-left:30px;}
.tab-a li a{color:#999; font-size:22px; line-height:1; font-weight:200; border-bottom:2px solid transparent; padding-bottom:2px;}
.tab-a li.on a {color:#111; font-weight:400; border-bottom-color:#111;}

/*tooltip*/
.tooltip .tipCont{display:none}
.tooltip .tipIcon:focus + .tipCont{display:block}

/*selectbox*/
.selectbox > .cont{display:none}
.selectbox.open > .cont{display:block}

/*accordion*/
.accordion-title {position:relative; display:block; width:100%; text-align:left;}
.accordion-title.open + .accordion-panel{height:auto;}
.accordion-title {position:relative;}
.accordion-title > button{position:absolute;top:0;bottom:0;left:0;right:0;width:100%;height:100%;font-size:0;-webkit-tap-highlight-color:transparent;}
.accordion-title label{position:relative;z-index:2}
.accordion-title::after {
	content:''; position:absolute; top:50%; right:0; background:url('../img/icon_dropdown.png') no-repeat; 
	width:11px; height:6px;transition:0.4s; margin-top:-3px; cursor:pointer;
}
.accordion-title.open::after {transform:rotate( -180deg )}
.accordion-panel {overflow:hidden;height:0;transition:0.3s ease;}

/*ListGroup*/
.list-group li {padding-left:10px; font-size:14px; color:#666;}
.list-group li::before {float:left;  margin-left:-10px;}
.list-group.dash li::before {content:'-'}
.list-group.dot li::before {content:'•'}
.list-group.reference li {padding-left:20px;}
.list-group.reference li::before {content:'※'; margin-left:-20px;}

/*ratio*/
.ratio {position:relative;}
.ratio > * {position:absolute; top:0; left:0; width:100%; height:100%; object-fit:cover;}
.ratio::before{content:''; display:block; /*padding-top:var(--aspect-ratio);*/}
.ratio > img {display:block;}
.ratio-1x1::before {padding-top:100%}
.ratio-4x3::before {padding-top:calc(3 / 4 * 100%);}
.ratio-16x9::before {padding-top:calc(9 / 16 * 100%);}
.ratio-21x9::before {padding-top:calc(9 / 21 * 100%);}


/*spinner*/
.prd-spinner {display:inline-block; background-color:#fff; border:1px solid #c7c7c7; height:30px; border-radius:6px; padding:0 12px; font-size:0;}
.prd-spinner input {border:none; height:28px; text-align:center; width:35px; font-size:14px; color:#111; font-weight:600;}
.prd-spinner .btn-plus, .prd-spinner .btn-minus {font-size:0; width:10px; height:28px; background-position:center center; background-repeat:no-repeat; vertical-align:top;}
.prd-spinner .btn-plus {background-image:url('../img/spinner_plus.png');}
.prd-spinner .btn-minus {background-image:url('../img/spinner_minus.png');}

/*Unit(product)*/
.prd-list {display:flex; align-items:flex-start; flex-wrap:wrap;}
.prd-list li{position:relative;  }
.prd-list li > a {display:block; text-decoration:none; color:#111;}
.prd-list .badge-group {position:absolute; z-index:5;}
.prd-list .badge {display:inline-block; background:#111; color:#fff; font-weight:300; font-size:12px; border-radius:6px;}
.prd-list .badge.babaon {background:#fc2264;}
.prd-list .brand {font-size:12px;color:#999; }
.prd-list .prd-img {position:relative; overflow:hidden; z-index:0; }
.prd-list .prd-img::before{content:''; position:absolute; top:0; left:0; width:100%; height:100%; background:rgba(17,17,17,0.03); z-index:3;}
.prd-list .prd-img .ratio {z-index:1;}
.prd-list .prd-img .ratio::before{padding-top:calc(421 / 288 * 100%);}
.prd-list .name { line-height:1.43; margin-top:10px;}
.prd-list .name .tx-ovf {display:block; white-space: nowrap;overflow: hidden;text-overflow: ellipsis;}
.prd-list .prd-img .pre-order {
	position:absolute; z-index:5; color:#fff; bottom:0; left:0; 
	width:100%; text-align:center; font-size:18px; line-height:1; padding:9px 0;
	background:linear-gradient(to bottom, rgba(17,17,17,0), rgba(17,17,17,0.3));
}
.prd-list .prd-img .pre-order span {display:block; font-size:14px; margin-bottom:4px;}
.prd-list .prd-img .pre-order strong {font-size:20px; font-weight:600;}
.prd-list .prd-price {display:flex; align-items:center; flex-wrap:wrap; font-size:12px; margin-top:16px;}
.prd-list .price {font-weight:400;}
.prd-list .price > strong {font-weight:500;}
.prd-list .discount {color:#fc2264; font-weight:500; margin-left:7px;}
.prd-list .price-org {color:#999; text-decoration:line-through; margin-left:7px;}
.prd-list .price-special {font-weight:500; font-size:12px; margin-top:9px;}
.prd-list .price-special > strong {font-weight:500;}
.prd-list .premium {color:#a38853;}
.prd-list .member {color:#111;}
.prd-list .secret {color:#fc2264;}
.prd-list .btn-wish {position:absolute; z-index:4; top:20px; right:15px; display:inline-block; width:22px; height:18px; font-size:0; background:url('../img/btn_wish.png') no-repeat;}
.prd-list .btn-wish.on {background-position-y:-20px;}
.prd-list .tag-group{font-size:0; margin-top:13px;}
.prd-list .tag {display:inline-block; vertical-align:top; font-size:10px; color:#666; background-color:#eee; line-height:1; padding:7px 5px 6px; border-radius:4px;}
.prd-list .tag + .tag {margin-left:5px;}
.prd-list .rank {
	position:absolute; z-index:5; top:15px; left:15px; width:44px; height:44px; text-align: center; line-height:44px;
	font-family: 'Montserrat'; font-weight:600; font-size:20px; color:#fff; background:#111; border-radius: 6px; padding:0 18px;
}

.prd-list.thum .badge-group{top:15px; left:15px;}
.prd-list.thum .badge{ height:30px; line-height:30px; padding:0 15px;}
.prd-list.thum .prd-info {padding:16px 0 0;}
.prd-list.thum li + li {margin-left:20px;}

/*Colume 6*/
.prd-list.col-6 li{width:calc((100% - 100px) / 6); }
.prd-list.col-6 li:nth-child(n+7) {margin-top:50px;}
.prd-list.col-6 li:nth-child(6n+1) {margin-left:0;}
/*Colume 5*/
.prd-list.col-5 li{width:calc((100% - 80px) / 5); }
.prd-list.col-5 li:nth-child(n+6) {margin-top:50px;}
.prd-list.col-5 li:nth-child(5n+1) {margin-left:0;}

.prd-list.list {}
.prd-list.list li {width:33.3333%;}
.prd-list.list li > a {display:flex; align-items:stretch;}
.prd-list.list .prd-img {width:120px; margin-right:20px;}
.prd-list.list .prd-info {flex:1; display:flex; flex-direction:column; align-content:flex-start; }
.prd-list.list .badge-group{top:10px; left:10px;}
.prd-list.list .badge{height:22px; line-height:22px; padding:0 10px;}
.prd-list.list .tag-group {align-items:flex-end; margin-top:auto;}

.prd-list.col-4 {width:100%;}
.prd-list.col-4 li {width:464px;}
.prd-list.col-4 li:nth-child(n+5) {margin-top:50px;}
.prd-list.col-3 li {width:414px;}
.prd-list.col-3 li:nth-child(n+4) {margin-top:50px;}

.swiper-container .prd-list {flex-wrap: inherit;}
.swiper-container .prd-list.thum li + li {margin-left:0;}

/*swiper*/
.swiper-wrap .swiper-container{padding-bottom: 50px;}
.swiper-wrap .swiper-button-next, 
.swiper-wrap .swiper-button-prev {
	display:block; color:#111; width:40px; height:40px; top:calc((288 / 421 * 100%) / 2); text-align:center; 
}
.swiper-wrap .swiper-button-next::after, 
.swiper-wrap .swiper-button-prev::after {font-size:24px; line-height:40px;}
.swiper-wrap .swiper-button-prev {left:0;}
.swiper-wrap .swiper-button-next {right:0;}
.swiper-button-next, 
.swiper-button-prev  {opacity: 0;}
.swiper-wrap :hover .swiper-button-next, 
.swiper-wrap :hover .swiper-button-prev  {opacity: 1; }
.swiper-wrap .swiper-scrollbar {background:#c7c7c7;}
.swiper-wrap .swiper-scrollbar-drag {background:#111; border-radius: 0;}
.swiper-wrap .swiper-container-horizontal>.swiper-scrollbar {left:0; bottom:0; height:1px; width:100%;}
.swiper-wrap .swiper-container .prd-list.thum li {margin-top:0!important;}




.prd-unit .unit-img a{display:block;} 
.prd-unit .unit-title {font-weight: 500; line-height: 1.25; font-size:32px; color:#111; margin-bottom: 30px;}
.prd-unit .prd-list {min-width:unset; max-width: none; flex-wrap: nowrap; }
.prd-unit .prd-list li {width:calc((100% - 28px) / 3)!important;}
.prd-unit .prd-list .brand {white-space: nowrap; overflow: hidden; text-overflow: ellipsis;}
.prd-unit .prd-list .name {display:-webkit-box; -webkit-line-clamp:2; -webkit-box-orient:vertical; overflow:hidden; text-overflow:ellipsis;}
.prd-unit .swiper-wrap {overflow: hidden;}
.prd-unit .swiper-button-next, 
.prd-unit .swiper-button-prev {top:124px;}
.prd-unit.sm .swiper-button-next, 
.prd-unit.sm .swiper-button-prev {top:86px;}
.prd-unit.horizontal {display:flex; align-items: stretch; justify-content: space-between; }
.prd-unit.horizontal .unit-img,
.prd-unit.horizontal .unit-box {flex-shrink:0;width:calc((100% - 20px) / 2);}
.prd-unit.horizontal:not(.sm) .swiper-wrap {height:calc(100% - 70px); overflow: hidden;}
.prd-unit.horizontal.sm .unit-title{margin-bottom:20px;}
.prd-unit.horizontal.sm .prd-list.thum .prd-info{padding-top:12px;}
.prd-unit.horizontal.sm .prd-list.thum .name {margin-top:0;}
.prd-unit.vertical .unit-img{margin-bottom:30px;}
.prd-unit.vertical .swiper-container{padding-bottom:55px;}
.prd-unit.sm .prd-list .discount {width:100%; margin-left:0; margin-top:7px;}

.swiper-wrap .swiper-button-next.swiper-button-disabled, 
.swiper-wrap .swiper-button-prev.swiper-button-disabled{opacity:0;}


.prd-list.cart li {position:relative; display:flex; align-items:stretch; justify-content:space-between; padding:0 0 30px; width:100%;}
.prd-list.cart li + li:not(.gifts) {background:url('../img/line_cart.png') repeat-x; padding-top:30px;}
.prd-list.cart li + li.gifts {margin-top:-10px;}
.prd-list.cart .checkbox {width:20px; margin:0 10px; align-self:center;}
.prd-list.cart .prd-img {width:104px;}
.prd-list.cart .prd-img .ratio::before { padding-top:calc(156 / 104 * 100%);}
.prd-list.cart .prd-info {flex:1; display:flex; flex-direction:column; align-content:flex-start;  padding-left:30px; }
.prd-list.cart .discount {color:#111;}
.prd-list.cart .delivery {position:absolute; top:56px; right:0; font-size:14px; color:#111;}
.prd-list.cart .free-ship {position:absolute; top:29px; right:0;  color:#999; font-size:14px; line-height:20px;}
.prd-list.cart .brand {padding-top:3px; font-size:14px; color:#111111;}

.prd-list.cart .ly-tooltip {left:auto; right:-2px;}

.tooltip {position: relative; display:inline-block; vertical-align: top; }
.btn-tooltip {font-size:0; display:inline-block; vertical-align:top; background:url('../img/icon_notice.png') no-repeat; width:20px; height:20px; }
.tooltip h3 {font-weight:400; font-size:12px; margin-bottom:10px; color:#fff; line-height:1;}
.ly-tooltip {opacity: 0; position:absolute;  top:25px; left:-5px;  background:#111;  padding:10px; border-radius: 4px; min-width:186px; transition:0.3s ease; z-index:-3;}
.tooltip-conts {color:#fff; line-height:1.4; font-size:12px; font-weight:300;}
.btn-closed {font-size:0; width:28px; height:28px; position: absolute; top:0; right:0; background:url('../img/btn_tooltip_closed.png') center center no-repeat; }
.btn-tooltip.on + .ly-tooltip {opacity: 1; z-index:3;}


.prd-list.cart .brand + .name{margin-top:11px;}
.prd-list.cart .name {margin:0;}
.prd-list.cart .option {color:#999; font-size:14px; margin-top:4px;}
.prd-list.cart .v-bottom {display:flex; align-items:flex-end; justify-content:space-between; align-content: flex-end; margin-top:auto;}
.prd-list.cart .prd-price {flex-wrap: wrap; font-size:14px; margin-top:0;}
.prd-list.cart .price-special {font-size:14px; margin-top:0px;}
.prd-list.cart .price-special  strong{font-weight:500;}
.prd-list.cart .prd-price + .prd-price {margin-top:8px;}
.prd-list.cart .prd-price.disabled *{color:#a8a8a8!important;}
.prd-list.cart .prd-price.disabled strong {font-weight: 400;}
.prd-list.cart .btn-wish {top:0px; right:35px;}
.prd-list.cart .btn-delete {position:absolute; top:0px; right:0; background:url('../img/btn_closed.png') center center no-repeat; width:20px; height:20px; font-size:0;}
.prd-list.cart li + li .btn-wish,
.prd-list.cart li + li .btn-delete {top:30px;}
.prd-list.cart li + li .free-ship {top:59px;}
.prd-list.cart li + li .delivery {top:86px;}
.prd-list.cart .notice {margin-top:8px; color:#fc2264; font-size:14px; font-weight:500;}
.prd-list.cart .price-org {margin-left:10px;}
.prd-list.cart li.disabled .prd-img {opacity:0.6;}
.prd-list.cart li.disabled .prd-info > *:not(.v-bottom ),
.prd-list.cart li.disabled .v-bottom .prd-price {opacity:0.6;}
.prd-list.cart .total {margin-top:8px; color:#999; font-size:14px;}
.prd-list.cart .total > strong {font-weight:600;}
.prd-list.cart .btn-vitical .btn + .btn {margin-top:10px;}

.price-list dl{display:flex; align-items:center; justify-content:space-between; font-size:16px;}
.price-list dl + dl {margin-top:12px;}
.price-list dt {color:#666; }
.price-list dd {color:#111; text-align:right; }
.price-list dl dd > strong {font-weight:500;}
.price-list dl.total {margin-top:27px; }
.price-list dl.total dt{color:#111; font-weight:500;}
.price-list dl.total dd {font-weight:400; font-size:24px;}
.price-list dl.total .discount {color:#fc2264; font-size:16px;}
.price-list dl.total .discount > strong {font-weight: 500;}


/*product - top*/
.prd-top { display:flex; align-items: center; justify-content: space-between; border-bottom:1px solid #f2f2f2; padding-bottom:20px;  margin-bottom: 40px;}
.prd-top .total {font-size:12px; color:#111; font-family: 'Montserrat';}
.prd-top select {border:none; padding-right:20px; font-size:12px; background:url('../img/form_select_sm.png') 100% 50% no-repeat;}
.prd-top select + select {margin-left:20px;}


/*Coupon*/
.coupon {
	display:inline-block;
	vertical-align:top;
	width:380px;
	background:url('../img/coupon_bg.png') no-repeat;
	box-shadow:10px 15px 23px rgba(7,7,7,0.15);
}
.coupon .inner-box {display:flex; align-items:center; height:158px; padding:20px 25px;}
.coupon .radio {width:100%;}
.coupon input[type=radio] {left:25px; top:50%; margin-top:-10px;}
.coupon label::before {margin-right:10px;}
.coupon .inner-box .info {flex:1; font-size:16px; color:#111; font-weight:400; }
.coupon .discount {width:110px; font-size:24px; font-weight:700;}
.coupon .discount > strong {font-size:40px; font-weight:900;}
.coupon .name {font-weight:500;}
.coupon .date {font-weight:500; margin-top:4px;}
.coupon .txt {font-size:14px; color:#999; line-height:1.4285; margin-top:14px;}
.coupon.notused {background-position-y:-160px; box-shadow:none;}
.coupon.notused .inner-box {height:60px;}
.coupon .btn-down {font-size:0; width:50px; height:158px; background:#000 url('../img/icon_down_w.png') center center no-repeat; margin-right:-25px; border-radius:0 10px 10px 0;}

/*Paging*/
.paging {display:flex; align-items: center; justify-content: center; margin-top:80px;}
.paging a {text-decoration: none; color:#111; font-size:14px; font-weight:300; margin:0 15px;}
.paging .on {font-weight:600;}
.paging .prev , 
.paging .next {width:14px; height:14px; background-position: center center; background-repeat: no-repeat; margin:0;}
.paging .prev span, .paging .next span {font-size:0;} 
.paging .prev {background-image:url('../img/btn_page_prev.png'); margin-right:30px;}
.paging .next {background-image:url('../img/btn_page_next.png'); margin-left:30px;}


/*design scroll*/
