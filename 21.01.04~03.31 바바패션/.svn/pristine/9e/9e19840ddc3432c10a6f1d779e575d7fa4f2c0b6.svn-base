@charset "utf-8";

.for-a11y{display: block;z-index: -1;height: 1px;overflow: hidden;font-size: 1px;line-height: 1px;color: transparent;border: 0;padding: 0;opacity: 0;-ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=0)";filter: alpha(opacity=0);background: none;position: absolute;width: 1px;margin: 0;}

.btn-basicXS{height: 20px;min-width: 30px;border-radius:10px;padding:0 10px;font-size: 10px;}
.btn-basicXS.fill{background: #111;border-color: #111;color: #fff;}

.form-flex{display: flex;}
.form-flex .form-flex-cell{flex:1}
.form-flex .form-flex-cell + div{margin-left: 10px;}

.tab-e{}
.tab-e > ul{display:flex;padding:5px 0;}
.tab-e > ul > li:not(:first-child){margin-left:28px}
.tab-e > ul > li{font-weight:200;color:#d0d0d0;margin-bottom:-1px}
.tab-e > ul > li > a{display:block;padding:10px 0 7px 0;font-weight: 300;}
.tab-e > ul > li.on{font-weight:400;color:#000}
.tab-e > ul > li.on > a{border-bottom:2px solid #000;font-weight: 500;}

.expand{margin:0 -20px;padding:0 20px}

/* 배송지 입력 */
/* .shippingAddress{padding: 10px 0 0} */
.shippingAddress .btnWrap{margin-top: 20px;}
.shipping-input{}
.shipping-input > div + div{margin-top: 10px;}
.shipping-check{margin-top: 20px;}
.shipping-check > div + div{margin-top: 20px;}
.chk-agree{font-size: 12px;vertical-align:top;display: inline-block;}
.chk-agree span{vertical-align: middle;}

/* 배송지 목록 */
.shippingListTop{padding:0 20px 20px;margin:0 -20px;border-bottom:15px solid #f5f5f5}
.shipping-list-cont{color:#111;position: relative;padding-top: 20px;}
.shipping-list-cont .name{font-size: 14px;font-weight: 500;margin-right: 40px;}
.shipping-list-cont .phone{font-size: 12px;font-weight: 500;margin-top: 10px;}
.shipping-list-cont .address{font-size: 12px;margin-top: 10px;}
.shipping-list-cont .btns{font-size: 0;text-align: right;margin-top: 15px;}
.shipping-list-cont .btns button{font-size: 12px;}
.shipping-list-cont .btns button + button{margin-left: 10px;}
.shipping-list-cont .delete{position: absolute;right: 0;top: 20px;}
.shipping-list-cont .delete button{font-size: 12px;font-weight: 500;color: #999;}

.shippingListBottom{}
.shippingListBottom .shipping-list-cont{padding-bottom: 20px;}
.shippingListBottom .shipping-list-cont + .shipping-list-cont{border-top:1px solid #eee}

.title-wrap{position: relative;}
.title-wrap button{position:absolute;right: 0;top: 50%;margin-top:-15px;height:30px;font-size: 11px;padding:0 15px}

/* 중복할인쿠폰 */
.dcCouponWrap{}
.dcCouponWrap .coupon-add{padding:20px 10px 0;}
.dcCouponWrap .tip-wrap{padding:20px 10px 0;}
.tip-wrap{}
.tip-wrap .tip + .tip{margin-top: 10px;}

.coupon-wrap{background:#f5f5f5;padding:20px 20px 80px;margin:20px -20px -20px}
.coupon-wrap .btnWrap{margin-top: 20px;}
.coupon-list li + li{margin-top: 15px;}
.discount-coupon{position: relative;}
.discount-coupon > input[type="radio"]{position: absolute;left: 20px;top: 50%;margin-top: -10px;z-index: 1;}
.discount-coupon .discount-coupon-inner{display:block;border-radius: 8px;position: relative;background: url(../img/bg_coupon.png) no-repeat 0 0;background-size: 100% 100%;}
.discount-coupon .discount-coupon-inner .flex-box{display: flex;align-items:stretch}
.discount-coupon .discount-coupon-inner .flex-box .dc-rate{margin-left: 20px;width: 80px;color: #111;font-weight: 700;font-size: 0;display:flex;align-items:center}
.discount-coupon .discount-coupon-inner .flex-box .dc-rate .num{font-size: 35px;}
.discount-coupon .discount-coupon-inner .flex-box .dc-rate .unit{font-size: 16px;position: relative;top:5px}
.discount-coupon .discount-coupon-inner .flex-box .info{flex:1;margin: 0 10px;padding:22px 0}
.discount-coupon .discount-coupon-inner .flex-box .info .tit{font-size: 14px;color: #111;font-weight: 500;}
.discount-coupon .discount-coupon-inner .flex-box .info .date{font-size: 12px;color: #111;margin-top: 5px;}
.discount-coupon .discount-coupon-inner .flex-box .info .desc{font-size: 12px;color: #999;font-weight: 300;margin-top: 5px;}
.discount-coupon .discount-coupon-inner .flex-box .download{width:40px;}
.discount-coupon .discount-coupon-inner .flex-box .download button{background:#111 url(../img/ico_download.png) no-repeat 50% 50%;background-size:14px 12px;width:100%;height:100%;border-radius:0 6px 6px 0;}

.discount-coupon label.discount-coupon-inner .flex-box .dc-rate{margin-left: 50px;}
.discount-coupon label.discount-coupon-inner .flex-box .info{margin-right: 20px;}
.discount-coupon > input[type="radio"]:checked + .discount-coupon-inner{filter: drop-shadow(2px 3px 8px #b7b7b7);}
.discount-coupon .discount-coupon-inner.none{background: url(../img/bg_coupon_none.png) no-repeat 0 0;background-size: 100% 100%;}
.discount-coupon .discount-coupon-inner.none .flex-box > div{flex:auto;margin-left:50px;padding:17px 0;font-size: 12px;}

.dcCouponWrap .chk-maximum{font-size: 12px;padding-top: 20px;}
.dcCouponWrap .chk-maximum span{vertical-align: middle;}

.no-data{text-align: center;font-size: 14px;color: #999;position: relative;padding: 120px 0;}
/* .empty ~ div{display: none;} */
.no-data .txt{display: block;margin-top: 15px;}
.no-data:before{content:'';display: inline-block;width:34px;height:34px;vertical-align:top;background: red;}

/* 통합검색 */
.search-area{position: relative;}
.search-area .search-input[type="text"]{background: #e9e9e9;height:50px;border-bottom: none;border:1px solid #e9e9e9;border-radius:50px;padding:10px 60px 10px 20px;}
.search-area .search-input[type="text"]:focus{border: 1px solid #eee;background: #fff;}
.search-area .search-btn{width: 50px;height: 50px;border-radius:50px;position: absolute;right: 0;top: 0;background: #333;}
.search-area .search-btn:before{content:'';display:inline-block;width:16.5px;height:16px;background: url(../img/ico_search.png) no-repeat 0 0;background-size: 100% auto;}

.realtime-keyword{background: #f5f5f5;/*padding-top: 25px;*/padding-top: 10px;padding-bottom: 25px;}
.realtime-keyword dl{}
.realtime-keyword dl dt{display: table;width: 100%;padding:14px 0}
.realtime-keyword dl dt span{display:table-cell;vertical-align:middle;color: #111;font-size: 16px;font-weight: 500;}
.realtime-keyword dl dt em{display:table-cell;vertical-align:middle;width:120px;font-size: 11px;color: #999;text-align: right;}
.realtime-keyword dl dd{margin-top: 10px;}
.realtime-keyword ul{}
.realtime-keyword ul li{}
.realtime-keyword ul li + li{margin-top: 12px;}
.realtime-keyword ul li a{display: flex;align-items:center;}
.realtime-keyword ul li a span{}
.realtime-keyword ul li a span.rank-num{font-size: 14px;font-weight: 600;width:28px;color:#999}
.realtime-keyword ul li:nth-child(1) a span.rank-num,
.realtime-keyword ul li:nth-child(2) a span.rank-num,
.realtime-keyword ul li:nth-child(3) a span.rank-num{color:#111;}
.realtime-keyword ul li a span.rank-thumb{width:40px}
.realtime-keyword ul li a span.rank-thumb img{width:100%;}
.realtime-keyword ul li a span.rank-keyword{font-size: 14px;margin-left: 10px;flex:1;}
.realtime-keyword ul li a span.rank-change{width:30px;text-align: right;}
.realtime-keyword ul li a span.rank-change span{font-size: 11px;}
.realtime-keyword ul li a span.rank-up{color:#fc2264;}
.realtime-keyword ul li a span.rank-up:before{content: '';display: inline-block;width:10px;height:8.5px;margin-right:5px;background: url(../img/ico_rankup.png) no-repeat 0 0;background-size: 100%;}
.realtime-keyword ul li a span.rank-down{color:#666;}
.realtime-keyword ul li a span.rank-down:before{content: '';display: inline-block;width:10px;height:8.5px;margin-right:5px;background: url(../img/ico_rankdown.png) no-repeat 0 0;background-size: 100%;}
.realtime-keyword ul li a span.rank-n{}
.realtime-keyword ul li a span.rank-n:before{content:'';display:inline-block;width:10px;height:1px;background: #666;vertical-align: super;position:relative;left:-10px;}

.hash-tag-list{font-size: 0;margin-top:-10px}
.hash-tag-item{border:1px solid #e3e3e3;font-size: 14px;color: #333;margin:10px 10px 0 0;padding:10px 15px;border-radius:40px;min-width: 70px;}
.hash-tag-item:before{content:'#';}

/* 스크롤배너 */
.banner-has-scroll{padding-top: 35px;}
.banner-has-scroll .swiper-container{padding:0 0 20px}
.banner-has-scroll img{width:100%;}
.banner-has-scroll .swiper-scrollbar{bottom:0;border-radius:0;height:1px;background: #c7c7c7;}
.banner-has-scroll .swiper-scrollbar .swiper-scrollbar-drag{background: #111;}

/* 2단 배너 */
.banner-type2{padding-top: 24px;padding-bottom: 30px;}
.banner-type2 .swiper-container{padding:0 0 30px}
.banner-type2 img{width: 100%;}
.banner-type2 .swiper-pagination{bottom:7px;line-height:0;}
.banner-type2 .swiper-pagination .swiper-pagination-bullet{opacity:1;background:#b7b7b7}
.banner-type2 .swiper-pagination .swiper-pagination-bullet-active{background: #111;}