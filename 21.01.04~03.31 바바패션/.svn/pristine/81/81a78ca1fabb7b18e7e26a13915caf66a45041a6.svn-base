@charset "utf-8";
/* templete */
.expand{margin:0 -20px;padding:0 20px}
.expand-slider{margin:0 -20px;}
.for-a11y{display: block;z-index: -1;height: 1px;overflow: hidden;font-size: 1px;line-height: 1px;color: transparent;border: 0;padding: 0;opacity: 0;-ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=0)";filter: alpha(opacity=0);background: none;position: absolute;width: 1px;margin: 0;}

.btn-basicXS{min-width: 30px;border-radius:10px;padding:2px 10px;font-size: 10px;display: inline-block;vertical-align: middle;height:auto;max-height:20px;}
.btn-basicXS.fill{background: #111;border-color: #111;color: #fff;}

.layerPop.full-size .cont{margin:0; width:100vw; height:100vh; overflow-y:auto;}
.layerPop.full-size .btn-close {
    bottom:auto; top:12px; right:16px; font-size:0; width:32px; height:32px;
    background:url('../img/layerPopClosed.png') center center no-repeat; background-size:16px 16px;
}
.layerPop.full-size .layer-title {font-size: 18px; font-weight: 300; line-height: 1.56; letter-spacing: -0.12px; padding:14px 56px 14px 20px; color:#000;}
.layerPop.full-size .cont > section{padding:0 20px 20px;}

.form-input{}
.form-input > li + li{margin-top: 10px;}

.form-input + .accordionBox{margin-top: 20px;}
.form-input + .message-box{margin-top: 30px;}

.form-flex{display: flex;}
.form-flex .form-flex-cell{flex:1}
.form-flex .form-flex-cell + div{margin-left: 20px;}
.form-flex .form-flex-select{width:110px;}
.form-flex .form-flex-btn{width:110px;}
.form-flex .form-flex-btn button{width: 100%;}
[class *= 'btn-basic'].fill{background: #111;color: #fff;}
.verify-num{position: relative;}
.verify-num input{padding-right:164px;}
.verify-num span{font-size: 14px;color: #fc2264;position: absolute;right:120px;top:14px;}
.verify-num button{position: absolute;top: 8px;right: 0px;height: 30px;line-height: 30px;padding: 0 15px;background: #e9e9e9;color: #666;min-width: 80px;border-radius: 4px;font-size: 11px;}
.verify-message{font-size: 12px;color: #fc2264;margin-top: 10px;}

.form-check{margin-top: 20px;}
.form-check > li{display: flex;justify-content: space-between;align-items:flex-start}
.form-check > li + li{margin-top: 20px;}
.form-check > li > button{font-size: 12px;color: #666;}
.form-check + .term-txt{margin-top: 10px;}

.term-txt{color:#111;}
.term-txt + .alarm-chk{margin-top: 10px;}

.chk-agree{font-size: 12px;vertical-align:top;display: inline-block;color:#111;position: relative;}
.chk-agree input[type="checkbox"]{position: absolute;left: 0;top: 0;}
.chk-agree span{display:inline-block;margin-left: 24px;}
.chk-agree .tip{display:inline-block;font-size: 12px;margin-top:3px;}

.accordionBox{width: 100%;}

.fullWidth{width: 100%;}

.message-box{background: #f5f5f5;font-size: 12px;padding: 15px;}
.message-box-tit{color: #111;}
.message-box-txt{color: #666;margin-top: 20px;}

.banner-has-scroll{/*padding-top: 35px;*/}
.banner-has-scroll .swiper-container{/*padding:0 0 20px*/}
.banner-has-scroll img{width:100%;}
.banner-has-scroll .swiper-scrollbar{bottom:0;border-radius:0;height:1px;background: #c7c7c7;margin-left: 20px;margin-right: 20px}
.banner-has-scroll .swiper-scrollbar .swiper-scrollbar-drag{background: #111;}

.card-unit{}

.title-area{padding:30px 0 25px 0}
.title-area-inner{display: flex;justify-content:space-between;align-items:center;}
.title-area-inner .title{font-size:18px;color: #111;font-weight: 700;}
.title-area-inner .title-extra{font-size: 12px;color: #666;}

.banner-type2{}
.banner-type2 .swiper-container{padding:0 0 30px}
.banner-type2 img{width: 100%;}
.banner-type2 .swiper-pagination{bottom:7px;line-height:0;}
.banner-type2 .swiper-pagination .swiper-pagination-bullet{opacity:1;background:#b7b7b7}
.banner-type2 .swiper-pagination .swiper-pagination-bullet-active{background: #111;}

.accordion-panel > div > .form-check{margin-top: 10px;}

.tip-wrap{margin-top: 10px;}
.tip-wrap .tip + .tip{margin-top: 5px;}
.tip-wrap + .form-input{margin-top: 10px;}
.tip.type2{padding-left: 8px;}
.tip.type2:before{content:'·';}
.tip.type3{padding-left: 8px;}
.tip.type3:before{content:'-';}
