/* 로그인 */
.loginWrap {/* padding:40px 20px 56px; */padding:10px 0 0}
.login-title {font-size: 20px;color:#111; font-weight:300;margin-top: 15px;}
.login-title .point{font-weight: 400;}
.login-subTitle,
.login-subText{font-size: 12px; color:#111; margin-top:16px;}

.login-title + *,
.login-subTitle + *,
.login-subText + *{margin-top: 30px;}

