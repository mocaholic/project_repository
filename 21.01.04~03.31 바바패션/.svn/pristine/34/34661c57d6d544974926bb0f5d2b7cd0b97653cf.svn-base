<script>
    $(document).ready(function(){
        /*publishing UI only*/
        $('.btn-wish').on('click', function(){
                    $(this).toggleClass('on');
        });// wish button toggle Class
        
        $('.category-tab .btn-more').click(function(){
            if($(this).hasClass('on') === true) {
                $(this).removeClass('on');
                $('.ctgr-menu').css('height','33px');
            } else {
                $(this).addClass('on');
                $('.ctgr-menu').css('height','76px');
            }
        });//카테고리 tab 메뉴 더보기 버튼 
    });  
</script>
