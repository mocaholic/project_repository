                <li class="swiper-slide on"><a href="#">프리미엄</a></li>
                <li class="swiper-slide"><a href="#">혜택</a></li>
            </ul>

        </div>
        <script>
            var tab_a_swiper_sample = new Swiper('#tab-a-swiper-sample', {
                slidesPerView: 'auto',
                resistance : false,
                slidesOffsetBefore: 20,
                slidesOffsetAfter: 20,
            });
        </script>
