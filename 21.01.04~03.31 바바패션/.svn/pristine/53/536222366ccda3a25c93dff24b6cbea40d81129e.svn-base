.c-grayE3{color:#E3E3E3;}
.c-grayE{color:#EEE;}

.c-bgf5 {background-color:#f5f5f5;}
.c-bgff {background-color:#fff;}

.c-primary {color:#a38853;}
.c-secondary {color:#fc2264}

/*Grid*/
.grid {display:flex; flex-wrap:wrap; justify-content:stretch;}
.w-full {width:100%;}
.w-half {width:50%;}
.col-3 {width:33.33333%}
.col-4 {width:25%}
.col-5 {width:20%}

/*Align*/
.align-c {text-align:center;}
.align-l {text-align:left;}
.align-r {text-align:right;}

/*float*/
.float-l {float:left;}
.float-r {float:right;}

/*flex*/
.d-flex {display:flex; align-items:stretch; justify-content:space-between;}
.align-items-center {align-items:center;}
.align-items-start {align-items:flex-start;}
.align-items-end {align-items:flex-end;}

.justify-centent-center {justify-content:center;}
.justify-centent-start {justify-content:flex-start;}
.justify-centent-end {justify-content:flex-end;}


/***Layout***/
.wrap {min-width:1320px; max-width:1920px; margin:0 auto; }
.container {color:#111; font-size:14px; font-weight:400; }

/*page-title*/
.page-title {color:#111; font-size:30px;}
.page-navi {float:right; font-size:0; margin-top:-26px;}
.page-navi > li {display:inline-block; vertical-align: top; font-size:14px; color:#999; font-weight:300;}
.page-navi > li + li::before {content:'>'; color:#999; margin:0 10px;}
.page-navi > li:last-child {font-weight: 400; color:#666;}


/*form(Input)*/
.form-box {position: relative; height:50px; border-bottom:1px solid #c7c7c7;}
.form-box.focus-line {border-bottom-color:#111;}
.form-box label {position:absolute; top:4px; left:10px; font-size:12px; color:#999;  transition-duration:0.3s;}
.form-box input,
.form-box select{font-size:16px; color:#111; padding-left:10px; padding-right:10px; width:100%; border:none; line-height:1; }
.form-box.disabled {background-color:#f5f5f5;}
.form-box.disabled :disabled {color:#a8a8a8;}
.form-box select {padding-top:25px; padding-bottom:8px; }
.form-box input {padding-top:20px;}

.form-box.focus label{font-size:16px; color:#111; font-weight:400; top:13px; }
.form-box.focus input {padding-top:12px}

.form-inp-group {display:flex; flex-wrap: wrap; align-items: center; padding:0 10px; height:50px; border-bottom:1px solid #c7c7c7;}
.form-inp-group input {border:none; width:120px;}
.form-inp-group .form-desc-txt {flex:1; padding:0 10px;}
.form-inp-group.disabled {background-color: #f5f5f5; color:#a8a8a8;}

.checkbox {position: relative; display:inline-block; vertical-align: top;}
.checkbox input[type=checkbox] {position:absolute; top:0; left:0; width:20px; height:20px; opacity:0;}
.checkbox label{line-height:20px; }
.checkbox label:not(.only)::before {margin-right:10px;}
.checkbox label.only {font-size:0;}
.checkbox label::before {content:''; display:inline-block; vertical-align: top; width:20px; height:20px; background:url('../img/form_checkbox.png') no-repeat;}
.checkbox :checked + label::before {background-position-y: -20px;}
.checkbox :disabled + label::before {background-position-y: -40px;}
.checkbox :checked:disabled + label::before {background-position-y: -60px;}

.radio {position: relative; display:inline-block; vertical-align: top;}
.radio input[type=radio] {position:absolute; top:0; left:0; width:20px; height:20px; opacity:0; cursor: pointer;}
.radio label{line-height:20px; cursor: pointer;}
.radio label:not(.only)::before {margin-right:10px;}
.radio label.only {font-size:0;}
.radio label::before {content:''; display:inline-block; vertical-align: top; width:20px; height:20px; background:url('../img/form_radio.png') no-repeat;}
.radio :checked + label::before {background-position-y: -20px;}
.radio :disabled + label::before {background-position-y: -40px;}
.radio :checked:disabled + label::before {background-position-y: -60px;}

.form-input{  padding-top:20px;}
.form-input label {position: absolute; top:21px; left:10px; font-size:16px; color:#111;}
.form-input input {border:none; width:100%; line-height:1; padding:0 10px; font-size:16px; color:#111;}
.form-input.disabled input {background-color: transparent; }
.form-desc-msg {display:none; width:100%; font-size:14px; color:#111; line-height:1;  }

.form-error-msg {display:none;}
.form-desc-msg.active, .form-error-msg.active {display:block; margin-top:10px;}


/*buttons*/
[type=button]:not(:disabled), [type=reset]:not(:disabled), [type=submit]:not(:disabled), button:not(:disabled) {cursor: pointer;}
.btn {
	display:inline-block; text-align: center; text-decoration: none; border:1px solid transparent; 
	font-size:18px; border-radius: 6px; height:40px; min-width:120px; padding:0 20px; color:#666; border-color:#c7c7c7;
}
.btn:not(.btn-form).disabled,
.btn:not(.btn-form):disabled {background-color: #f5f5f5; color:#a8a8a8; }
.btn.sm{min-width: 80px; height:30px; font-size:12px; padding-left:10px; padding-right:10px;}
.btn.lg {min-width: 350px; height:60px; padding:0 30px; border-radius: 30px;}
.btn.primary {border-color:#111; color:#111;}
.btn.secondary {border-color:#fc2264; color:#fc2264;}
.btn.lg.primary {background-color:#111; color:#fff; box-shadow:14px 10px 24px rgba(7,7,7,0.26);}
.btn.lg.disabled, .btn.lg:disabled {border-color:transparent;}
.btn.btn-form {color:#666; border-color: transparent; background-color: #e3e3e3; padding-left:20px; padding-right:20px;}

/*table*/
.tb-default {width:100%; border-top:1px solid #111;}
.tb-default th,
.tb-default td {border-bottom:1px solid #e3e3e3; text-align:left;font-size:14px; color:#666; padding:18px 20px; }
.tb-default th {background-color:#f5f5f5; font-weight:400; }

/*LayerPopup*/
.layerPop{position:fixed;top:0;left:0;right:0;bottom:0;width:100%;height:100%;z-index:100;background:rgba(0,0,0,.5);transform:translateX(-100%);visibility:hidden;display:flex;justify-content:center;align-items:center;overflow:hidden;}
.layerPop .cont{position:relative;background:#fff;width:100%;margin:0;min-height:10rem;}
.layerPop .btn-close{position:absolute;bottom:0;right:0}
.layerPop.open{transform:translateX(0);visibility:visible;}

/*tab*/
.tab-ui ul{display:flex}
.tab-ui li a{display:flex;justify-content:center;align-items:center;height:40px;min-width:80px}
.tab-ui li.on a{background:#000;color:#fff}
.tab-cont > div{display:none}
.tab-cont > div.on{display:block}

/*tooltip*/
.tooltip .tipCont{display:none}
.tooltip .tipIcon:focus + .tipCont{display:block}

/*selectbox*/
.selectbox > .cont{display:none}
.selectbox.open > .cont{display:block}

/*accordion*/
.accordion-title {background-color: #eee;color: #444;cursor: pointer;padding: 18px;width: 100%;text-align: left;border: none;outline: none;transition: 0.4s}
/*.active, .accordion:hover {background-color: #ccc}*/
.accordion-panel {padding: 0 18px;background-color: white;overflow: hidden;height: 0;transition: .3s ease;}

/*ListGroup*/
.list-group li {padding-left:10px;}
.list-group li::before {float:left;  margin-left:-10px;}
.list-group.dash li::before {content:'-'}
.list-group.dot li::before {content:'•'}
.list-group.reference li {padding-left:20px;}
.list-group.reference li::before {content:'※'; margin-left:-20px;}

/*ratio*/
.ratio {position:relative;}
.ratio-1x1{--aspect-ratio:100%;}
.ratio-4x3{--aspect-ratio:calc(3 / 4 * 100%);}
.ratio-16x9{--aspect-ratio:calc(9 / 16 * 100%);}
.ratio-21x9{--aspect-ratio:calc(9 / 21 * 100%);}
.ratio::before{content:''; display:block; padding-top:var(--aspect-ratio);}
.ratio > * {position:absolute; top:0; left:0; width:100%; height:100%; object-fit:cover;}

/*spinner*/
.prd-spinner {display:inline-block; background-color:#fff; border:1px solid #c7c7c7; height:30px; border-radius: 15px; padding:0 12px; font-size:0;}
.prd-spinner input {border:none; height:28px; text-align:center; width:35px; font-size:14px; color:#111; font-weight:600;}
.prd-spinner .btn-plus, .prd-spinner .btn-minus {font-size:0; width:10px; height:28px; background-position: center center; background-repeat: no-repeat; vertical-align:top;}
.prd-spinner .btn-plus {background-image:url('../img/spinner_plus.png');}
.prd-spinner .btn-minus {background-image:url('../img/spinner_minus.png');}

/*Unit(product)*/
.prd-list li{position:relative; }
.prd-list a {text-decoration: none; color:#111;}
.prd-list .badge-group {position:absolute;}
.prd-list .badge {display:inline-block; width:25px; height:25px; border-radius: 50%; font-size:12px; font-weight:600; text-align:center; line-height:25px;}
.prd-list .badge.new {background-color:#fff; color:#111; box-shadow: 4px 6px 10px rgba(0,0,0,0.04);}
.prd-list .badge.best {background-color:#111; color:#fff;}
.prd-list .btn-wish {position:absolute; z-index:4; display:inline-block; width:20px; height:20px; font-size:0; background:url('../img/btn_wish.png') no-repeat;}
.prd-list .btn-wish.on {background-position-y: -20px;}
.prd-list .prd-img {position:relative; }
.prd-list .prd-img::before{content:''; position:absolute; top:0; left:0; width:100%; height:100%; background:rgba(0,0,0,0.03);}
.prd-list .prd-price {display:flex; align-items: center; flex-wrap: wrap;}
.prd-list .price {font-weight:400;}
.prd-list .price > strong {font-weight:500;}
.prd-list .discount {font-weight:500; margin-left:7px;}
.prd-list .tag-group{font-size:0;}
.prd-list .tag {display:inline-block; vertical-align: top; font-size:12px; color:#666; background-color:#eee; line-height:1; padding:4px 10px; border-radius: 4px;}
.prd-list .tag + .tag {margin-left:5px;}
.prd-list .price-org {font-size:14px; color:#999; text-decoration: line-through;}

.prd-list.thumnail {overflow: hidden;}
.prd-list.thumnail li {float:left; width:188px; }
.prd-list.thumnail .badge-group {top:15px; left:15px; z-index:4; width:25px;}
.prd-list.thumnail .badge + .badge {margin-top:8px;}
.prd-list.thumnail .discount {}
.prd-list.thumnail .prd-img {height:282px; overflow: hidden;}
.prd-list.thumnail .prd-info {margin-top:13px;}
.prd-list.thumnail .prd-price {margin-top:18px;}
.prd-list.thumnail .name {line-height:1.428; }
.prd-list.thumnail .brand + .name {margin-top:10px;}
.prd-list.thumnail .discount {color:#fc2264;}
.prd-list.thumnail .price-org {width:100%; margin-top:6px;}
.prd-list.thumnail .tag-group {margin-top:13px;}
.prd-list.thumnail .btn-wish {right:0; bottom:54px;}

.prd-list.cart li {position:relative; display:flex; align-items: stretch; justify-content: space-between; padding:30px 0;}
.prd-list.cart li + li {background:url('../img/line_cart.png') repeat-x;}
.prd-list.cart .checkbox {width:20px; margin:0 10px; align-self:center;}
.prd-list.cart .prd-img {width:104px;}
.prd-list.cart .prd-info {flex:1; padding-left:30px; display:flex; flex-direction:column; align-content: flex-start;  }
.prd-list.cart .cart-row {display:flex; align-items: center; justify-content: space-between;}
.prd-list.cart .cart-row > *:first-child {width:325px;}
.prd-list.cart .v-bottom {flex: 1; align-items: flex-end;}
.prd-list.cart .btn-wish {top:30px; right:35px;}
.prd-list.cart .btn-delete {position:absolute; top:30px; right:0; background:url('../img/btn_closed.png') center center no-repeat; width:20px; height:20px; font-size:0;}
.prd-list.cart :disabled {opacity: 0.6;}
.prd-list.cart .brand {padding:3px 60px 0 0;}
.prd-list.cart .name {margin-top:13px;}
.prd-list.cart .delivery {margin-top:13px; color:#999; font-size:14px;}
.prd-list.cart .option {color:#999; font-size:14px; margin-top:7px;}
.prd-list.cart .notice {margin-top:8px; color:#fc2264; font-size:14px; font-weight:500;}
.prd-list.cart .price-org {margin-left:10px;}
.prd-list.cart li.disabled .prd-img,
.prd-list.cart li.disabled .prd-info > *:not(.v-bottom),
.prd-list.cart li.disabled .prd-price{opacity: 0.6;}