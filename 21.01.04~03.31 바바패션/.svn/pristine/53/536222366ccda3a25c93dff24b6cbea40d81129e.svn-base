/*Colors*/
.c-white {color:#fff;}
.c-black {color:#000;}
.c-gray1 {color:#111;}
.c-gray3 {color:#333;}
.c-gray6 {color:#666;}
.c-gray9{color:#999;}
.c-gray9{color:#999;}
.c-gray9{color:#999;}
