.findAlpabet li > a > strong{font-weight:500;font-size:14px}
.findAlpabet li > a > span{margin-left:8px;font-size: 12px}

.bottomToast{position:fixed;left:0;right:0;bottom:0;display:flex;height:64px;background:#000;color:#fff;z-index: 4;}
.bottomToast .reset{width:64px;height:64px;background:#222;color:#fff}
.bottomToast > strong{display:flex;flex:1;justify-content:flex-end;align-items:center;padding:5px 24px;font-weight:300}

.selectUI-a{overflow:hidden}
.selectUI-a .inner{display:flex;flex-wrap:wrap;margin: -3px;}
.selectUI-a label {min-width: calc(50% - 8px);max-width: calc(50% - 8px);min-height: 48px;box-sizing: border-box;margin: 3px;}
.selectUI-a.n3 label {min-width: calc(33.3% - 8px);height:48px;box-sizing: border-box;margin: 4px;}
.selectUI-a label > input{display:none}
.selectUI-a label > span{display: flex;flex-wrap: wrap;justify-content: center;align-items: center;width: 100%;height: 100%;border:1px solid #eaeaea;box-sizing: border-box;text-align: center;font-size: 14px;padding: 14px 6px;}
.selectUI-a label > span > i{letter-spacing: -1px;}
.selectUI-a label > span > i > em{font-size:12px}
