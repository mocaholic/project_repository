.selectUI-color label > input{display:none}
.selectUI-color label > span > em{position:relative;display:block;width: 32px;height: 32px;border-radius:50%;box-sizing: border-box;margin: 5px auto 5px auto;}
.selectUI-color label > input:checked + span{font-weight: 600;color:#b1863c;}
.selectUI-color label > input:checked + span:after{content:"";position:absolute;left: calc(50% - 20px);top: -4px;display:block;width: 40px;height: 40px;border-radius:50%;border: 1px solid #b1863c;box-sizing: border-box;}


.filterSelectVeiw{display:flex;white-space: nowrap;/* overflow-x: scroll; *//* overflow-y: hidden; */}
.filterSelectVeiw > .inner{/* padding: 0 20px; */display: flex;}
.filterSelectVeiw > .inner > span{display:inline-block;height:28px;line-height:28px;border-radius:14px;/* padding: 0 12px; *//* border: 1px solid; */font-size:14px;/* margin:6px; */vertical-align: middle;/* margin: 4px 12px; */font-weight: 700;width: auto;}
.filterSelectVeiw > .inner > span:not(:first-child){/* margin-left: 24px; */}
