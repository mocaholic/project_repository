.searchBox-a > .dropdownBox > a > strong > em{color:#b1863c}
.searchBox-a > .dropdownBox > a > span{margin-left:8px;font-weight:300}
.searchBox-a.open{box-shadow: 0 2px 8px 0 rgba(0, 0, 0, 0.12);}
