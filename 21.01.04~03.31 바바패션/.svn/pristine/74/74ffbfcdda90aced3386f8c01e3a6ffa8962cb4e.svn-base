.layerPop-filter{position:fixed;top:0;left:0;right:0;bottom:0;}
.layerPop-filter:before{content:"";display:block;/* background: #f8f8f8; */border-right: 1px solid #e8e8e8;width: 75px;height:100%;position: absolute;z-index: 0;}
.layerPop-filter .header{position:relative;z-index: 2;background: #111;color: #fff;/* box-shadow:0 2px 4px 0 rgba(0, 0, 0, 0.06); */}
.layerPop-filter h1{font-size: 24px;font-weight: 700;/* height:56px; *//* line-height:56px; */padding: 44px 20px 10px 20px;}
.layerPop-filter > .cont{position:relative;z-index: 1;height: calc(100% - 173px);padding-bottom: 71px;box-sizing: border-box;}
.layerPop-filter > .cont > ul > li > button{position: relative;width:120px;height:60px;color:#999;font-size:14px;text-align:left;padding:0 20px;background: #f8f8f8;box-sizing:border-box;z-index: 2;}
.layerPop-filter > .cont > ul > li > input[type='radio'] {display:none}
.layerPop-filter > .cont > ul > li > label{display: inline-flex;align-items: center;justify-content: center;position: relative;width: 75px;height:60px;/* line-height: 60px; */color:#999;font-size:14px;text-align: center;padding: 0 5px;/* background: #f8f8f8; */box-sizing:border-box;cursor: pointer;z-index: 2;}
.layerPop-filter > .cont > ul > li > input:checked + label{background:#fff;color:#000;font-weight:600}
.layerPop-filter > .cont > ul > li > input:checked + label:after{display:inline-block;width:1px;height: 19px;content:"";position: absolute;right:0;top: calc(50% - 9px);background: #111;}
/* .layerPop-filter > .cont > ul > li > input:checked + label:after{content:"✓";margin-left:7px} */
.layerPop-filter > .cont > ul > li > input:checked + div{display:block;}
.layerPop-filter > .cont > ul > li > label > input{display:none}
.layerPop-filter > .cont > ul > li > div{display:none}
.layerPop-filter > .cont > ul > li.on > div{display:block}
.layerPop-filter > .cont > ul > li.on > button{background:#fff;color:#000;font-weight:600}
.layerPop-filter > .cont > ul > li.on > button:after{content:"✓";margin-left:7px}
.layerPop-filter > .cont > nav{position: absolute;z-index: 2;width: 75px;height: 100%;overflow-y: scroll;}
.layerPop-filter > .cont > nav > label{position:relative;display: flex;height: 76px;align-items: center;justify-content: center;font-size: 13px;}
.layerPop-filter > .cont > nav > label.on:after{content:"";display:inline-block;width:1px;height:20px;background:#111;position:absolute;right:0;top: calc(50% - 9px);}
.layerPop-filter > .cont > .section{position: relative;height:100%;}
.layerPop-filter .btn-close{position:absolute;right: 10px;top: 47px;width:32px;height:32px;background: url(../img/close3.png) no-repeat center;background-size: 16px auto;text-indent:-10000px;z-index: 3;}
.layerPop-filter .filerTab-cont{position: absolute;top:0;bottom: 0;padding: 20px 20px 20px 95px;width: 100%;height: 100%;overflow-y: auto;box-sizing: border-box;}


