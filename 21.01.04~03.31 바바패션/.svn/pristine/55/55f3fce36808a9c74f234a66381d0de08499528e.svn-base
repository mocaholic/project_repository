            } else {
                $(this).addClass('active').text('알림받기');
            }
        });
    });
    var swiperLive = new Swiper('.babalive-wrap .swiper-container', {
        scrollbar: {
            el: '.babalive-wrap .swiper-scrollbar',
            dragSize: '200px',
        },
        slidesPerView: 4,
        spaceBetween: 20,
        navigation: {
            nextEl: '.babalive-wrap .swiper-button-next',
            prevEl: '.babalive-wrap .swiper-button-prev',
        },
    });
</script>
