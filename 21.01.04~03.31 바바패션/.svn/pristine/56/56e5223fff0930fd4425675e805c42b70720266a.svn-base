<script>
    /*publishing UI only*/
    $('.tooltip').find('.btn-tooltip').on('click', function(){
        $(this).toggleClass('on')
    }); // tooltip toggle Class
    $('.tooltip').find('.btn-closed').on('click', function(){
        $(this).closest('.tooltip').find('.btn-tooltip').removeClass('on')
    }); // tooltip Closed

    /*swiper*/
    var swiper = new Swiper('.evt-banner .swiper-container', {
        slidesPerView: 1,
        navigation: {
            nextEl: '.evt-banner .swiper-button-next',
            prevEl: '.evt-banner .swiper-button-prev',
        },
    });
</script>
