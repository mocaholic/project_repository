header .util-menu li + li {margin-left:20px;}
header .util-menu li a {display:block; font-size:13px; color:#111; line-height: 1.46; text-decoration:none;}
header .util-menu li a .badge {display:inline-block; vertical-align:top; min-width:16px; height:16px; line-height: 16px;  text-align: center; border-radius: 8px; background:#fc2264; margin:0 0 0 2px; padding:0 5px;font-size:11px; color:#fff; font-weight: 500;}
header .form-search {width:420px; border-bottom-color: #111;}
header .form-search input[type=text] {font-size: 14px; height:40px;}
header .form-search .btn-search {height:40px;}
header .form-search ::placeholder{color:#666; font-weight: 400;}


.header-top {display:flex; justify-content:space-between; align-items:center; padding:25px 0 11px;}
.header-gnb {display:flex; align-items: center; width:100%; padding:25px 0 10px;}
.gnb {display:flex; align-items: center;}
.gnb ul {display:flex; align-items: center;}
.gnb li + li  {margin-left:30px;}
.gnb li > a {font-size: 20px; color:#111; line-height: 1.45; letter-spacing: -0.6px; text-decoration: none; }
.gnb:not(.gnb-left) li > a{font-weight:300; padding-bottom:10px; border-bottom: 1px solid transparent;}
.gnb:not(.gnb-left) li > a:hover,
.gnb:not(.gnb-left) li > a:focus {font-weight:500;}
.gnb:not(.gnb-left) li.on > a {border-bottom-color: #111;}
.gnb-left li > a {font-weight:400;}
.gnb .on-air {display:inline-block; vertical-align: top; font-size: 12px; margin:3px 0 0 4px; line-height:1;
	font-weight: 500; color:#fff; background:#111; border-radius: 4px; padding:4px 8px; min-width:60px; text-align: center;}

.gnb-left + .gnb::before {content:''; display:inline-block;  width:1px; height:15px; background-color: #c7c7c7; margin:0 30px;}
.gnb:not(.gnb-left) + .gnb {margin-left:60px;}

header.fixed {position:fixed; left:0; top:0; z-index:99; background:#fff; width:100%; transform: translateY(-77px); border-bottom-color:#eee;}
header.fixed + .container {padding-top:200px;}
