html.bottomFloating-on body{padding-bottom: 84px;}


html{font-family: 'Montserrat';background: #fff;}
html,body,h1,h2,h3,h4,h5,h6,ul,li,dl,dt,dd,p,span{margin:0;padding:0}


body > section{min-height:calc(100vh - 120px);padding-left: 20px;padding-right: 20px;}
body > section > .member-benefit,
body > section > .unitList{margin-left: -20px;margin-right: -20px;}
body > section > * > .tab-b{margin-left:-20px;margin-right:-20px}


li{list-style:none}
button{cursor:pointer; border:0; font-family: 'Montserrat'; background-color:transparent; padding:0; margin:0;}
i,em{font-style: normal;}
*{outline:none}
a{text-decoration:none;color:inherit}
img{vertical-align:top}
strong{font-weight:700}

button:disabled{pointer-events:none}
::placeholder{color: #999;}
input{font-family: 'Montserrat';}
input[type="text"],input[type="password"]{height:45px;border:0;border-bottom: 1px solid #e3e3e3;width:100%;padding: 10px;box-sizing: border-box;}
input[type="text"]:disabled,input[type="password"]:disabled{background:#f5f5f5}
input[type="text"]:focus,input[type="password"]:focus{border-bottom:1px solid #111}

input[type='checkbox']{appearance: none;-webkit-appearance: none;width: 17px;height: 17px;/* border: 1px solid #111; */vertical-align: middle;border-radius: 2px;margin: 0;cursor: pointer;background: #e3e3e3 url(../img/check.png) no-repeat center;background-position-y: -10px;background-size: 10px auto;}
input[type='checkbox']:checked{background-position-y: 4px;background-color: #fff;border: 1px solid #111;}
label input[type='checkbox']{margin-right: 6px;vertical-align: middle;}
label input[type='checkbox'] + span{vertical-align:middle;}
input[type='checkbox'].type2{background:#e3e3e3;border:1px solid #e3e3e3;width:20px;height:20px;border-radius: 4px;background: #e3e3e3 url(../img/check2.png) no-repeat center 4px;background-size: 10px auto;background-position-y: -10px;}
input[type='checkbox'].type2:checked{border: 1px solid #111;background-position-y: 5px;background-color: #111;}

input[type='radio']{appearance: none;-webkit-appearance: none;width: 20px;height: 20px;border: 1px solid #111;vertical-align: middle;border-radius: 50%;margin: 0;cursor: pointer;background: url(../img/check-radio.png) no-repeat center;background-position-y: -10px;background-size: 10px auto;}
input[type='radio']:checked{background-position-y: 5px;}
label input[type='radio']{margin-right:4px;display: inline-flex;vertical-align: middle;}
input[type='radio'].type2{border:1px solid #e3e3e3;width:20px;height:20px;border-radius: 50%;background: #e3e3e3 url(../img/check2.png) no-repeat center 4px;background-size: 10px auto;background-position-y: -10px;}
input[type='radio'].type2:checked{border: 1px solid #111;background-position-y: 5px;background-color: #111;}

input[type='checkbox'].check-like{border:0;width:19px;height: 17px;background:url(../img/check-like.png);background-size: 19px auto;}
input[type='checkbox']:checked.check-like{background-position-y: -17px;}

select{height:46px;padding: 7px 30px 7px 10px;box-sizing:border-box;appearance:none;-webkit-appearance:none;background: url(../img/selectArr.png) no-repeat right 10px center;background-size: 11px auto;border: 0;border-bottom: 1px solid #c7c7c7;color: #999;width: 100%;}

table {border-spacing:0;border-collapse:collapse;width: 100%;}
table > caption {font-size:0; line-height:0; height:0;}
table th,table td{border: 1px solid #e3e3e3;padding: 10px 20px;height: 42px;box-sizing: border-box;}
table th{background:#f5f5f5;font-size:12px}
table td{background:#fff;font-size:10px;color:#666;text-align: center;}
table td:first-child,table th:first-child{border-left:0;}
table td:last-child,table th:last-child{border-right:0;}

/*ratio*/
[class *= 'ratio-'] {position:relative;}
[class *= 'ratio-']::before{content:'';display:block;padding-top:var(--aspect-ratio);}
[class *= 'ratio-'] > img {position:absolute;top:0;left:0;width:100%;/* height:100%; */object-fit:cover;}
.ratio-1x1{--aspect-ratio:100%;}
.ratio-4x3{--aspect-ratio: calc(4 / 3 * 100%);}
.ratio-16x9{--aspect-ratio:calc(9 / 16 * 100%);}
.ratio-21x9{--aspect-ratio:calc(9 / 21 * 100%);}
.ratio-a{--aspect-ratio: calc(16 / 10.943 * 100%);}

/*Align*/
.align-c {text-align:center;}
.align-l {text-align:left;}
.align-r {text-align:right;}

/* ui */
.tooltip .tipCont{display:none}
.tooltip .tipIcon:focus + .tipCont{display:block}

.selectbox > .cont{display:none}
.selectbox.open > .cont{display:block}

.title-a{position: relative;font-size: 16px;font-weight: 600;padding: 15px 0;}
.title-a:before{position:absolute;bottom:0;content:"";display:block;height:1px;width:100%;background: url(../img/dotted.gif) repeat-x;background-size:4px auto}
.title-a.payment{display:flex;justify-content:space-between;padding-right:35px}
.title-a.payment .price{color:#fc2264}
h2.required:after{content:"";display:inline-block;background:url(../img/req.png);width: 6px;height: 5px;background-size: 6px auto;vertical-align: top;margin: 4px 0 0 2px;}
.active, .accordion:hover {background-color: #ccc}
.accordion-panel {background-color: white;overflow: hidden;height: 0;transition: .3s ease;box-sizing: border-box;}
.accordion-title > .title-a:before{opacity:0;transition:.4s ease}
.accordion-title.open + .accordion-panel{height:auto;}
.accordion-title.open > .title-a:before{opacity:1}
.accordion-title {position: relative;/* padding: 0 20px; */}
.accordion-title > button{position:absolute;top:0;bottom:0;left:0;right:0;width:100%;height:100%;font-size: 0;-webkit-tap-highlight-color: transparent;}
.accordion-title:after{content:"";display:inline-block;position:absolute;top: calc(50% - 5px);right: 10px;background:url(../img/dropdownArr.png) no-repeat;width: 13px;height: 8px;background-size:13px auto;transform:rotate(180deg);pointer-events: none;}
.accordion-title.open:after{transform:rotate(0)}
.accordion-title label{position:relative;z-index:2}

.accordion-title.type2{background: #f5f5f5;padding: 15px 40px 15px 15px;font-size:12px;}
.accordion-title.type2:after{right:17px}
.accordion-title.type2 + .accordion-panel{/* padding-left:20px; *//* padding-right:20px; */}
.accordion-panel + .accordion-title.type2{margin-top:10px}
.accordion-panel > div{/* padding-top: 20px; *//* padding-bottom: 20px; */}



.layerPop{position:fixed;top:0;left:0;right:0;bottom:0;width:100%;height:100%;z-index:100;background: rgba(0,0,0,0);visibility: hidden;display: flex;justify-content: center;align-items: center;overflow: hidden;}
.layerPop > .cont{position:relative;background: #fff;width: 100%;margin: 1rem;min-height: 10rem;}
.layerPop > .cont > .btn-close{position:absolute;bottom:0;right:0}
.layerPop.open{visibility: visible;background: rgba(0,0,0,.5);}
.layerPop.bottomUp{transition: .5s ease;}
.layerPop.bottomUp > .cont{position: fixed;bottom: 0;margin: 0;border-radius: 30px 30px 0 0;padding: 20px;box-sizing: border-box;background: #f5f5f5;transform: translateY(100%);transition: .5s ease;}
.layerPop.bottomUp.open > .cont{transform:translateY(0)}
.tab-cont > div{display:none}
.tab-cont > div.on{display:block}

/*Header*/
header .sub-hedaer {display:flex; align-items: center; height:56px; padding:0 16px;}
header .page-title {flex:1; font-size:18px; color:#000; font-weight: 300; letter-spacing: -0.12px; line-height: 1; margin-left:12px;}
header button {display:inline-block;font-size:0;width:32px;height:32px;background-repeat:no-repeat;background-position: center center;background-size: contain;}
header button + button {margin-left:8px;}
header .btn-back {background-image:url('../img/headerBack.png');background-size: 14px auto;}
header .btn-search {background-image:url('../img/headerSearch.png');background-size: 19px auto;}
header .btn-cart {background-image:url('../img/headerCart.png');background-size: 18px auto;position:relative;}
header .btn-cart .num {position:absolute; top:0; right:0; min-width:14px; height:14px; border-radius: 7px; font-size:10px; text-align:center; line-height:14px; font-weight: 500; background-color:#db3126; color:#fff;}

.btn-share {position:fixed; right:20px; bottom:154px;  z-index:999; font-size:0; width: 40px; height: 40px; box-shadow: 0 6px 9px 0 rgba(0, 0, 0, 0.1); border: solid 1px #f0f0f0; border-radius: 50%;background:#fff url('../img/footerShare.png') center center no-repeat; background-size:18px 20px}
.btn-goTop {position:fixed; right:20px; bottom:94px;  z-index:999; font-size:0; width: 40px; height: 40px; box-shadow: 0 6px 9px 0 rgba(0, 0, 0, 0.1); border: solid 1px #f0f0f0; border-radius: 50%;background:#fff url('../img/footerGotop.png') center center no-repeat; background-size:14px 15px}

/*Footer*/
footer {background:#000; font-size:12px; color:#fff; line-height: 1.5; letter-spacing: -0.06px;}
footer .footer-menu {display:flex; justify-content:center;  background:#111; }
footer .footer-menu a {display:block; padding:12px 13px; font-size:12px; font-weight: 100; line-height:1; letter-spacing: -0.06px; color:rgba(255,255,255,0.7);}
footer .footer-info {padding:40px 20px 32px;}
footer .footer-logo {width:133px; }
footer .footer-logo img{display:block; width:100%;}
footer .footer-text {margin-top:20px; color:rgba(255,255,255,0.7); font-size:12px; font-weight: 100;}
footer .footer-text .underline {/*text-decoration: underline;*/ border-bottom:1px solid rgba(255,255,255,0.7);}
footer .footer-cs {display:flex; align-items: center; margin-top:32px; }
footer .footer-cs li + li::before {content:''; display:inline-block; width:1px; height:6px; background:rgba(255,255,255,0.2); margin:0 10px;}
footer .footer-cs a {font-size:10px; color:#999; }
footer .copyright {text-align:center;  font-weight: 300; color: #999; margin-top:32px;}
footer .sns-link {font-size:0; margin-top:32px;}
footer .sns-link li {display:inline-block; vertical-align: top;}
footer .sns-link li + li {margin-left:20px;}
footer .sns-link a{font-size: 12px;font-weight: 500; color:#fff;}

/*Bottom Floation*/
.bottomFloating {position:fixed; z-index:999; bottom:0; left:0; width:100%; height:64px; display:flex; align-items: stretch; box-shadow: 0 5px 20px 10px rgba(0, 0, 0, 0.07)}
.bottomFloating .btn {width:20%; display:block; font-size: 12px;line-height: 1; letter-spacing: -0.06px; text-align: center; color:#999; background:#fff;}
.bottomFloating .btn.home {color:#000;}
.bottomFloating .btn::before {content:''; display:block; width:17px; height:17px; margin:0 auto 8px; background-position: center center; background-repeat: no-repeat; background-size:contain}
.bottomFloating .btn.care::before{background-image:url('../img/botFloating01.png');}
.bottomFloating .btn.channel::before{background-image:url('../img/botFloating02.png');}
.bottomFloating .btn.home::before{background-image:url('../img/botFloating03.png');}
.bottomFloating .btn.mybaba::before{background-image:url('../img/botFloating04.png');}
.bottomFloating .btn.menu::before{background-image:url('../img/botFloating05.png');}