    script(type='text/javascript').
      var header = $('header.header_wrap');
      front.pageLoading(header, '../include/header.html', function() {
        
      });
      var footer = $('footer.footer_wrap');
      front.pageLoading(footer, '../include/footer.html', function() {/* callback */});
      $(function() {
        // 사무소위치 페이지 진입시 header 태그 class bg_white 추가
        $('header').addClass('bg_black');
        // tab 
        var btnCateTab = $('.office_cont .tab_menu > li > a');
        var tabContents = $('.office_cont .office_container');
        front.tabMenu(btnCateTab, 'on', tabContents, function($that) {
          
        });
        
        // 19.01.28 스크립트 수정
        
        (function($){
        	'use strict';
        	var _item, _target, index, speed;

        	function init(){
        		_item = $('.office_tab > li');
        		_target = _item.parent();
        		speed = 500;
        		bind();
        	};
        	function bind(){
        		_item.on('click', function(){
        			index = $(this).index();
        			var prevW = 0;
        			$(this).prevAll().each(function(){
        				prevW += $(this).outerWidth(true);
        			});
        			_target.animate({scrollLeft : prevW}, speed);
        		});
        	};
        	init();

        })($);
        
        // 검색결과 뉴스영역 menu 클릭시 
        var $btnAllNews = $('.btn_menu_tab > a');
        var $allMenuEle = $('.menu_tab_all');
        var fixedTarget = $('.fixed_mo_tab');
        
        
        front.btnClick($btnAllNews, function($this) {
          if ($this.hasClass('on')) {
            $('body').removeClass('hidden');
            $this.removeClass('on');
            $('.dimm_news.on').remove();
            $allMenuEle.removeClass('on');
          } else {
            front.scrollAni(fixedTarget, function() {
              $this.addClass('on');
              
              var btnAllMenus = $allMenuEle.find('> ul > li > a');
              front.btnClick(btnAllMenus, function($that) {
                var idx = $that.parent().index();
                btnCateTab.parent().removeClass('on');
                btnCateTab.parent().eq(idx).addClass('on');
                tabContents.find('> div').removeClass('on');
                tabContents.find('> div').eq(idx).addClass('on');
                $('.dimm_news.on').remove();
                $allMenuEle.removeClass('on');
                $btnAllNews.removeClass('on');
              });
              
              fixedTarget.append('<div class="dimm_news on"></div>').ready(function() {
                $(document).on('touchstart click', '.dimm_news', function() {
                  $('body').removeClass('hidden');
                  $this.removeClass('on');
                  $('.dimm_news.on').remove();
                  $allMenuEle.removeClass('on');
                });
              });
              $allMenuEle.addClass('on');
            });
          }
        });
        
        
      });
      
      
