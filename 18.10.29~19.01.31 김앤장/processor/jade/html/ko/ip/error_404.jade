// output: ../../../../../publish/html/ko/ip/error_404.html
include include/jadeUtil.jade
-var imgUrl = './images/dummy/'
doctype html
html(lang='ko')

  head
    +pageName('Kim & Chang')
    +metaCharset('utf-8')
    +metaName('viewport', 'width=device-width,initial-scale=1.0,maximum-scale=1.0,user-scalable=no')
    +httpEquiv('X-UA-Compatible', 'ie=edge')
    +metaName('robots', 'noydir, noodp, index, follow')
    +metaName('copyright', '© 2017 Kim & Chang. All Rights Reserved.')
    +metaName('description', '')
    +metaName('keywords', 'Kim & Chang')
    +metaProperty('og:title', 'Kim & Chang')
    +linkRel('stylesheet', 'text/css', './css/kimchang.css', 'all')
  body(style='height:100%;')
    .skip_navi
      a(href='#content') 본문 바로가기
    .error_wrap
      .inner
        h1.h1_error
          a(href='javascript:void(0);') kim&chang
        p.error_title 요청하신 페이지를 찾을 수 없습니다.
        .error_desc
          // 19.01.23 띄어쓰기 수정
          p 입력하신 주소가 정확한지 다시 확인 부탁드립니다.
          //p 불편사항은 <a href="mailto:lawkim@kimchang.com" class="text_orange">lawkim@kimchang.com</a>으로 연락 주시면 친절하게 안내해 드리겠습니다.
        .error_btn_wrap
          a(href='javascript:void(0);').btn_link.btn_hover.bg_black
            span 이전 페이지
          a(href='javascript:void(0);').btn_link.btn_hover.bg_black
            span 메인 화면
      
